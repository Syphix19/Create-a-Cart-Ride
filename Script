local a=loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()local b=a:NewWindow("Create a Cart Ride")local c=b:NewSection("Upgrades")local d=b:NewSection("Gamepasses")local e=b:NewSection("Other scripts")local f;c:CreateDropdown("Speed/Acceleration scale",{"1","2","3","Disable","Speed/Acceleration scale"},5,function(g)local f=g;if f=="1"then game:GetService("Players").LocalPlayer.PDF.SpeedUpgrade.Value=1 elseif f=="2"then game:GetService("Players").LocalPlayer.PDF.SpeedUpgrade.Value=2 elseif f=="3"then game:GetService("Players").LocalPlayer.PDF.SpeedUpgrade.Value=3 elseif f=="Disable"then game:GetService("Players").LocalPlayer.PDF.SpeedUpgrade.Value=0 end end)local h;c:CreateDropdown("Brake scale",{"1","2","Disable","Brake scale"},4,function(i)local h=i;if h=="1"then game:GetService("Players").LocalPlayer.PDF.BrakeUpgrade.Value=1 elseif h=="2"then game:GetService("Players").LocalPlayer.PDF.BrakeUpgrade.Value=2 elseif h=="Disable"then game:GetService("Players").LocalPlayer.PDF.BrakeUpgrade.Value=0 end end)local j;c:CreateDropdown("Track magnet",{"On","Off","Track magnet"},3,function(k)local j=k;if j=="On"then game:GetService("Players").LocalPlayer.PDF.TrackMagnets.Value=true;game:GetService("Players").LocalPlayer.PDF.TrackMagnetsOn.Value=true elseif j=="Off"then game:GetService("Players").LocalPlayer.PDF.TrackMagnetsOn.Value=false end end)e:CreateButton("Insta finish cart ride",function()loadstring(game:HttpGet("https://raw.githubusercontent.com/oceanhong970201/robloxscript/main/Create%20a%20Cart%20Ride.lua"))()end)local l;d:CreateToggle("Shift to sprint",function(m)local l=m;if l==true then game:GetService("Players").LocalPlayer.PDF.SprintShift.Value=true elseif l==false then game:GetService("Players").LocalPlayer.PDF.SprintShift.Value=false end end)
